{
  "name": "rainmaker",
  "version": "0.0.1",
  "main": "",
  "repository": "",
  "license": "egov",
  "private": true,
  "scripts": {
    "clean": "lerna clean -f --yes && rm -rf node_modules",
    "reset": "yarn run clean && yarn i",
    "dev:citizen": "cd packages/citizen && yarn start",
    "dev:employee": "cd packages/employee && yarn start",
    "prod:citizen": "yarn run go && cd packages/citizen && yarn run build",
    "prod:employee": "yarn run go && cd packages/employee && yarn run build",
    "transpile-citizen-modules": "run-p pgr-citizen pt-citizen",
    "transpile-employee-modules": "run-p pgr-employee pt-employee egov-hrms",
    "transpile-common-modules": "run-p egov-ui-kit egov-ui-framework egov-workflow egov-tradelicence",
    "egov-ui-kit-config": "babel dev-packages/egov-ui-kit-dev/src/config --out-dir packages/egov-ui-kit/config --copy-files",
    "egov-ui-kit-components": "babel dev-packages/egov-ui-kit-dev/src/components --out-dir packages/egov-ui-kit/components --copy-files",
    "egov-ui-kit-redux": "babel dev-packages/egov-ui-kit-dev/src/redux --out-dir packages/egov-ui-kit/redux --copy-files",
    "egov-ui-kit-utils": "babel dev-packages/egov-ui-kit-dev/src/utils --out-dir packages/egov-ui-kit/utils --copy-files",
    "egov-ui-kit-commons": "babel dev-packages/egov-ui-kit-dev/src/common --out-dir packages/egov-ui-kit/common --copy-files",
    "egov-ui-kit-custom-icons": "babel dev-packages/egov-ui-kit-dev/src/custom-icons --out-dir packages/egov-ui-kit/custom-icons --copy-files",
    "egov-ui-kit-hocs": "babel dev-packages/egov-ui-kit-dev/src/hocs --out-dir packages/egov-ui-kit/hocs --copy-files",
    "egov-ui-kit-assets": "babel dev-packages/egov-ui-kit-dev/src/assets --out-dir packages/egov-ui-kit/assets --copy-files",
    "egov-ui-kit": "babel dev-packages/egov-ui-kit-dev/src --out-dir packages/egov-ui-kit --copy-files",
    "pgr-citizen": "babel dev-packages/pgr-citizen-dev/src --out-dir packages/pgr-citizen --copy-files",
    "pt-citizen": "babel dev-packages/pt-citizen-dev/src --out-dir packages/pt-citizen --copy-files",
    "pgr-employee": "babel dev-packages/pgr-employee-dev/src --out-dir packages/pgr-employee --copy-files",
    "pt-employee": "babel dev-packages/pt-employee-dev/src --out-dir packages/pt-employee --copy-files",
    "build-css-employee": "run-p build-css-egov-ui-kit build-css-egov-ui-framework-core build-css-egov-pgr-employee build-css-egov-pt-employee",
    "build-css-citizen": "run-p build-css-egov-ui-kit build-css-egov-ui-framework-core  build-css-pgr-citizen build-css-egov-pt-citizen",
    "build-css-egov-ui-kit": "node-sass-chokidar dev-packages/egov-ui-kit-dev/src -o dev-packages/egov-ui-kit-dev/src",
    "build-css-pgr-citizen": "node-sass-chokidar dev-packages/pgr-citizen-dev/src -o dev-packages/pgr-citizen-dev/src",
    "build-css-egov-ui-framework-core": "node-sass-chokidar dev-packages/egov-ui-framework-core/src -o dev-packages/egov-ui-framework-core/src",
    "build-css-egov-pgr-employee": "node-sass-chokidar dev-packages/pgr-employee-dev/src -o dev-packages/pgr-employee-dev/src",
    "build-css-egov-pt-employee": "node-sass-chokidar dev-packages/pt-employee-dev/src -o dev-packages/pt-employee-dev/src",
    "build-css-egov-pt-citizen": "node-sass-chokidar dev-packages/pt-citizen-dev/src -o dev-packages/pt-citizen-dev/src",
    "egov-ui-framework-redux": "babel dev-packages/egov-ui-framework-core/src/ui-redux --out-dir packages/egov-ui-framework/ui-redux --copy-files",
    "egov-ui-framework-utils": "babel dev-packages/egov-ui-framework-core/src/ui-utils --out-dir packages/egov-ui-framework/ui-utils --copy-files",
    "egov-ui-framework-config": "babel dev-packages/egov-ui-framework-core/src/ui-config/screens/specs --out-dir packages/egov-ui-framework/ui-config/screens/specs --copy-files",
    "egov-ui-framework-hocs": "babel dev-packages/egov-ui-framework-core/src/ui-hocs --out-dir packages/egov-ui-framework/ui-hocs --copy-files",
    "egov-ui-framework-atoms": "babel dev-packages/egov-ui-framework-core/src/ui-atoms --out-dir packages/egov-ui-framework/ui-atoms --copy-files",
    "egov-ui-framework-molecules": "babel dev-packages/egov-ui-framework-core/src/ui-molecules --out-dir packages/egov-ui-framework/ui-molecules --copy-files",
    "egov-ui-framework-containers": "babel dev-packages/egov-ui-framework-core/src/ui-containers --out-dir packages/egov-ui-framework/ui-containers --copy-files",
    "egov-ui-framework": "babel dev-packages/egov-ui-framework-core/src --out-dir packages/egov-ui-framework --copy-files",
    "egov-workflow": "babel dev-packages/egov-workflow-dev/src --out-dir packages/egov-workflow --copy-files",
    "egov-tradelicence": "babel dev-packages/egov-tradelicence-dev/src --out-dir packages/egov-tradelicence --copy-files",
    "egov-hrms": "babel dev-packages/egov-hrms-dev/src --out-dir packages/egov-hrms --copy-files",
    "go": "yarn && yarn run build-css-employee && yarn run build-css-citizen && yarn run transpile-common-modules && yarn run transpile-citizen-modules && yarn run transpile-employee-modules && lerna bootstrap"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "lerna": "^2.11.0",
    "run-p": "0.0.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5"
  }
}
